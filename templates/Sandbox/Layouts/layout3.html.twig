{# template racine (niveau 1) pour TwigController : version 3 #}
<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Sandbox{% endblock %}</title>
    </head>
    <body>
        <div id="palmares">
            {{ render(controller('App\\Controller\\Sandbox\\TwigController::palmaresAction', {n : palmares_size})) }}
        </div>

        <div id="vue">
            <h1>{% block titre_principal %}{% endblock %}</h1>
            {% block vue %}{% endblock %}
        </div>

        <div id="footer">
            {% include 'Sandbox/Layouts/_footer.html.twig' %}
        </div>
    </body>
</html>
