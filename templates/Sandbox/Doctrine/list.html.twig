{% extends 'Sandbox/Layouts/layout4.html.twig' %}
{% block title %}{{ parent() }} : doctrine{% endblock %}

{% block titre_principal %}Vue "list" : exercices Doctrine{% endblock %}


{% block vue %}
    <h2>Liste de tous les films</h2>
    <table>
        <tr><th>id</th><th>titre</th></tr>
        {% for film in films %}
             <tr>
                 <td>{{ film.id }}</td>
                 <td>{{ film.titre }}</td>
             </tr>
        {% endfor %}
    </table>

    <h2>dump</h2>
    {{ dump(films) }}
{% endblock %}